<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             android:layout_width="match_parent"
             android:layout_height="match_parent">

    <FrameLayout
            android:id="@+id/bg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/transparent">

    </FrameLayout>

    <com.kakaovx.homet.lib.page.PageGestureView
            android:id="@+id/gestureView"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <FrameLayout
                android:id="@+id/contents"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#eee">
            <Button
                    android:text="START"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/btnStrat" android:layout_gravity="bottom"/>
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                <com.kakaovx.homet.user.component.ui.view.graph.GraphCircle
                        android:layout_width="200dp"
                        android:layout_height="200dp" android:id="@+id/circle"
                        android:layout_gravity="center" android:layout_marginTop="5dp"/>
                <com.kakaovx.homet.user.component.ui.view.graph.GraphPolygon
                        android:layout_width="200dp"
                        android:layout_height="200dp" android:id="@+id/polygon"
                        android:layout_gravity="center" android:layout_marginTop="5dp"
                />
                <com.kakaovx.homet.user.component.ui.view.graph.GraphBar
                        android:layout_width="20dp"
                        android:layout_height="100dp" android:layout_gravity="bottom|center_horizontal"
                        android:id="@+id/bar"
                        android:layout_marginTop="5dp"/>
                <com.kakaovx.homet.user.component.ui.view.graph.GraphBar
                        android:layout_width="match_parent"
                        android:layout_height="20dp" android:layout_gravity="bottom|center_horizontal"
                        android:id="@+id/bar2"
                        android:layout_marginTop="5dp"/>
            </LinearLayout>

        </FrameLayout>
    </com.kakaovx.homet.lib.page.PageGestureView>

</FrameLayout>
