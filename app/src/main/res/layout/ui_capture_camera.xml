<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             android:layout_width="match_parent"
             android:layout_height="match_parent">
    <com.kakaovx.homet.user.component.ui.skeleton.view.AutoFitTextureView
            android:id="@+id/texture"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    <com.kakaovx.homet.user.component.ui.skeleton.view.OverlaySurfaceView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/surfaceView">
    </com.kakaovx.homet.user.component.ui.skeleton.view.OverlaySurfaceView>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnCapture"
            android:layout_gravity="bottom|center_horizontal"/>
    <Button
            android:text="back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnChangeCamera"/>
</FrameLayout>